使用registry v2(可以通过docker实现，也可以编译成一个二进制程序，直接运行)
#nohup ./registry server config.yml &
如下为config.yml配置文件内容
version: 0.1
log:
fields:
service: registry
storage:
cache:
blobdescriptor: inmemory
filesystem:
rootdirectory: /docker/registry
http:
addr: :5000
headers:
X-Content-Type-Options: [nosniff]
health:
storagedriver:
enabled: true
interval: 10s
threshold: 3
